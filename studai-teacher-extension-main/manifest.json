{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "Inject HTML and CSS into a webpage",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
